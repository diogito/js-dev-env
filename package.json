{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "custom javascript development environment",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel"
  },
  "author": "diogito",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "chalk": "2.4.1",
    "css-loader": "^0.28.11",
    "express": "4.16.3",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "style-loader": "^0.21.0",
    "webpack": "^4.9.1",
    "webpack-dev-middleware": "^3.1.3"
  }
}
